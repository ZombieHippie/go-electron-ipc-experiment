{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "build": "go build -o go-build/main.exe  -buildmode exe ./go-src/main.go",
    "build-start": "npm-run-all -s proto-build build start",
    "start": "electron src/main.js",
    "watch": "nodemon --ext go,js,html,proto --watch src --watch go-src --watch protobuf-src --exec npm run build-start -s",
    "proto-build": "protoc -I=./protobuf-src/ --go_out=./protobuf-build protobuf-src/go-search-request.proto protobuf-src/math-operation.proto"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atom/electron-quick-start.git"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/atom/electron-quick-start/issues"
  },
  "homepage": "https://github.com/atom/electron-quick-start#readme",
  "devDependencies": {
    "electron-prebuilt": "^0.36.0"
  },
  "dependencies": {
    "color": "^0.11.1",
    "npm-run-all": "^1.6.0",
    "protobufjs": "^5.0.1",
    "protocol-buffers": "^3.1.6"
  }
}
