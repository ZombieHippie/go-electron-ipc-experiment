{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "build": "go build -o go-build/main.exe  -buildmode exe ./go-src/main.go",
    "build-start": "npm-run-all -s build start",
    "start": "electron src/main.js",
    "watch": "nodemon --ext go,js,html --watch src --watch test --exec npm run build-start -s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atom/electron-quick-start.git"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/atom/electron-quick-start/issues"
  },
  "homepage": "https://github.com/atom/electron-quick-start#readme",
  "devDependencies": {
    "electron-prebuilt": "^0.36.0"
  },
  "dependencies": {
    "color": "^0.11.1",
    "npm-run-all": "^1.6.0"
  }
}
